[program:git_service]
environment=PYTHONPATH=/var/rtb-devops/classes
command = /usr/bin/python /var/rtb-devops/remote-services/git-zk.py

process_name=%(program_name)s
autostart=true
autorestart=true
stopsignal=KILL
stopasgroup = true
killasgroup = true


stdout_logfile=/tmp/supervisor_git_service.log
stdout_logfile_maxbytes=1MB
stdout_logfile_backups=1

stderr_logfile = /tmp/supervisor_git_service.err
stderr_logfile_maxbytes=1MB
stderr_logfile_backups=1

[group:git_service_server]
programs=git_service






