[program:do_firewall]
environment=PYTHONPATH=/var/rtb-devops/classes
command = /usr/bin/python /var/rtb-devops/remote-services/do_firewall.py
autostart=true
autorestart=true

stopsignal=KILL
stopasgroup = true
killasgroup = true

stdout_logfile=/tmp/do_firewall.log
stdout_logfile_maxbytes=1MB
stdout_logfile_backups=1

stderr_logfile = /tmp/do_firewall.err
stderr_logfile_maxbytes=1MB
stderr_logfile_backups=1

[group:do_firewall_server]
programs=do_firewall
