
[program:mab_monitor]
environment=PYTHONPATH=/var/rtb-devops/classes
command = /usr/bin/python /var/rtb-devops/remote-services/mab_monitor.py

process_name=%(program_name)s
autostart=true
autorestart=true
stopsignal=KILL
stopasgroup = true
killasgroup = true


stdout_logfile=/tmp/supervisor_mab_monitor.log
stdout_logfile_maxbytes=1MB
stdout_logfile_backups=1

stderr_logfile = /tmp/supervisor_mab_monitor.err
stderr_logfile_maxbytes=1MB
stderr_logfile_backups=1


[group:mab_monitor_server]
programs=mab_monitor





